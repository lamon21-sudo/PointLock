{
  "name": "pointlock",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.1.0",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "clean": "turbo clean && rm -rf node_modules",
    "dev:api": "turbo dev --filter=@pointlock/api",
    "dev:mobile": "turbo dev --filter=@pointlock/mobile",
    "db:generate": "pnpm --filter @pointlock/api db:generate",
    "db:migrate": "pnpm --filter @pointlock/api db:migrate",
    "db:push": "pnpm --filter @pointlock/api db:push",
    "db:studio": "pnpm --filter @pointlock/api db:studio"
  },
  "devDependencies": {
    "turbo": "^2.3.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "express-rate-limit": "^8.2.1"
  }
}
